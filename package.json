{
  "name": "dms-jbrowse-components",
  "version": "0.1.0",
  "description": "Jbrowse components for DMS",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/overture-stack/dms-jbrowse-components"
  },
  "scripts": {
    "build::assets": "cp ./package.json ./dist/package.json && cp ./README.md ./dist/README.md",
    "build::babel": "babel ./src --extensions .tsx,.ts,.js --out-dir ./dist",
    "build::ts": "tsc -p ./tsconfig.json",
    "build": "npm run clean && npm run build::assets && npm run declarations && npm run build::babel",
    "clean": "rm -rf dist && mkdir dist",
    "declarations": "ttsc --emitDeclarationOnly",
    "dev": "npm run watch",
    "publish-dms-jbrowse": "cd dist && npm publish --access public",
    "type-check": "tsc -p ./tsconfig.json",
    "watch": "npm run clean && npm run build::assets && (npm run build::babel -- --watch --source-maps=inline & npm run declarations -- --watch)"
  },
  "peerDependencies": {
    "react": "17.x.x",
    "react-dom": "17.x.x"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.21.5",
    "@jbrowse/react-circular-genome-view": "^2.4.2",
    "@jbrowse/react-linear-genome-view": "^2.4.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@babel/register": "^7.21.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.16.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "babel-loader": "^9.1.2",
    "babel-preset-env": "^1.7.0",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node-dev": "^2.0.0",
    "ttypescript": "^1.5.15",
    "typescript": "^5.0.4",
    "webpack": "^5.81.0"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "all",
    "singleQuote": true
  }
}
